<?xml version="1.0" encoding="utf-8" ?>
<Template id="Homepage" haspgtime="no">
  <Properties excludestd="">
    <Field name="HeadTitle" type="txtfield" label="Header title" />
    <Field name="SeoKeys" type="txtfield" label="SEO Keywords" />
    <Field name="SeoDesc" type="txtfield" label="SEO Description" />
    <Field name="MenuName" type="txtfield" label="Menu name" />   
  </Properties>
  <WidgetBox />
  <Widgets>
    <Widget ename="HomepageBanners" wid="WidgetListMgr" jsel="article.content" mgrtype="gallery" mgrattr="Banner:DesktopImage" cols="Image">
      <Widget ename="Banner" wid="Banner" DesktopImageAttr="cmgrw:114" AddFront="yes" />
    </Widget>
    
    <Widget ename="Introduction" wid="TwoTitleContentBlock" label="Title" jsel="div.intro" /> 
    
    <Widget ename="HomepageQuickLinks" wid="WidgetListMgr" jsel="div.links" mgrtype="txtlist" mgrattr="QuickLink:Text" cols="Text">
      <Widget ename="QuickLink" wid="TextLinkBlock" AddFront="yes" />
    </Widget> 
    
  </Widgets>
</Template>